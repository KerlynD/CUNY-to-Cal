{
  "name": "cuny-to-cal",
  "version": "1.0.0",
  "description": "Chrome extension to export CUNY schedule to calendar (.ics)",
  "main": "dist/manifest.json",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "zip": "node scripts/zip.js",
    "check-all": "npm run type-check && npm run lint && npm run test && npm run test:e2e",
    "ci-test": "node scripts/ci-test.js",
    "ci-test:ps1": "powershell -ExecutionPolicy Bypass -File scripts/ci-test.ps1",
    "ci-test:bash": "bash scripts/ci-test.sh",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "ics": "^3.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "archiver": "^6.0.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^21.5.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "keywords": [
    "chrome-extension",
    "cuny",
    "calendar",
    "ics",
    "schedule"
  ],
  "author": "CUNY-to-Cal Team",
  "license": "MIT"
}
